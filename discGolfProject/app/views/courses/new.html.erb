<h1>Add a course</h1>
	<form action="/courses/new" method="post">
		<input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
		<p>
			<label for="name">Name </label>
			<input type="text" name="course[name]">
		</p>
		<p>
			<label for="street">Street </label>
			<input type="text" name="course[street]">
		</p>
		<p>
			<label for="city">City </label>
			<input type="text" name="course[city]">
		</p>
		<p>
			<label for="state">State </label>
			<input type="text" name="course[state]">
		</p>
		<p>
			<label for="zip_code">Zip code </label>
			<input type="text" name="course[zip_code]">
		</p>
		<p>
			<label for="google_map">Google map link </label>
			<input type="text" name="course[google_map]">
		</p>
		<input type="submit" value="Add a course">
	</form>